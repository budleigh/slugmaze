{
  "name": "slugmaze",
  "version": "0.0.0",
  "description": "a slug in a maze",
  "main": "server.js",
  "engines": {
    "node": "6.2.1"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "prebuild": "npm run clean",
    "build:html": "cp src/index.html build/index.html",
    "build:js": "browserify src/index.js -t [ babelify --presets [ es2015 ] ] > build/bundle.js",
    "build": "npm run build:html && npm run build:js",
    "prewatch": "npm run clean",
    "watch": "npm run prebuild && watchify src/index.js -o build/bundle.js -v -t babelify",
    "start": "node server.js"
  },
  "keywords": [
    "slug",
    "maze"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "~6.10.1",
    "babel-preset-es2015": "~6.9.0",
    "watchify": "~3.7.0",
    "babelify": "~7.3.0",
    "browserify": "~13.0.1"
  },
  "dependencies": {
    "event-emitter": "^0.3.4",
    "express": "~4.14.0",
    "lodash": "~4.13.1",
    "tween.js": "^16.3.4"
  }
}
